{"ast":null,"code":"import axios from 'axios';\naxios.defaults.withCredentials = true;\nexport default {\n  data: () => ({\n    posts: {},\n    errors: [],\n    el: document.querySelector('.right_bar')\n  }),\n\n  created() {\n    console.log(123);\n    axios.post('http://127.0.0.1:8000/check_ses', {\n      user_sid: document.cookie\n    }, {\n      withCredentials: true\n    }).then(response => {\n      console.log(response);\n\n      if (String(response.data) === 'false') {\n        location.href = '/login';\n      }\n    }).catch(e => {\n      console.log(e);\n    });\n  },\n\n  methods: {\n    click_photo_d() {\n      this.el.css('display', 'none');\n      this.el.css('width', '100%');\n      this.el.css('border-radius', '30px 30px 30px 30px');\n    },\n\n    click_photo_u() {\n      this.el.css('display', 'block');\n      this.el.css('width', '389px');\n      this.el.css('border-radius', '30px 30px 30px 30px');\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA2EA,OAAOA,KAAP,MAAkB,OAAlB;AACAA,KAAK,CAACC,QAAN,CAAeC,eAAf,GAAiC,IAAjC;AACA,eAAe;EACbC,IAAI,EAAE,OAAO;IACXC,KAAK,EAAE,EADI;IAEXC,MAAM,EAAE,EAFG;IAGXC,EAAE,EAAEC,QAAQ,CAACC,aAAT,CAAuB,YAAvB;EAHO,CAAP,CADO;;EAMbC,OAAM,GAAK;IACTC,OAAO,CAACC,GAAR,CAAY,GAAZ;IACAX,KAAI,CACDY,IADH,CAEI,iCAFJ,EAGI;MACEC,QAAQ,EAAEN,QAAQ,CAACO;IADrB,CAHJ,EAMI;MACEZ,eAAe,EAAE;IADnB,CANJ,EAUGa,IAVH,CAUSC,QAAD,IAAc;MAClBN,OAAO,CAACC,GAAR,CAAYK,QAAZ;;MACA,IAAIC,MAAM,CAACD,QAAQ,CAACb,IAAV,CAAN,KAA0B,OAA9B,EAAuC;QACrCe,QAAQ,CAACC,IAAT,GAAgB,QAAhB;MACF;IACD,CAfH,EAgBGC,KAhBH,CAgBUC,CAAD,IAAO;MACZX,OAAO,CAACC,GAAR,CAAYU,CAAZ;IACD,CAlBH;EAoBD,CA5BY;;EA6BbC,OAAO,EAAE;IACPC,aAAY,GAAK;MACf,KAAKjB,EAAL,CAAQkB,GAAR,CAAY,SAAZ,EAAuB,MAAvB;MACA,KAAKlB,EAAL,CAAQkB,GAAR,CAAY,OAAZ,EAAqB,MAArB;MACA,KAAKlB,EAAL,CAAQkB,GAAR,CAAY,eAAZ,EAA6B,qBAA7B;IACD,CALM;;IAMPC,aAAY,GAAK;MACf,KAAKnB,EAAL,CAAQkB,GAAR,CAAY,SAAZ,EAAuB,OAAvB;MACA,KAAKlB,EAAL,CAAQkB,GAAR,CAAY,OAAZ,EAAqB,OAArB;MACA,KAAKlB,EAAL,CAAQkB,GAAR,CAAY,eAAZ,EAA6B,qBAA7B;IACF;;EAVO;AA7BI,CAAf","nam