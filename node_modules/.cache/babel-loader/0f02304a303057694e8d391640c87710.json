{"ast":null,"code":"import axios from 'axios';\nimport $ from 'jquery';\n\nfunction getCookie(name) {\n  const matches = document.cookie.match(new RegExp('(?:^|; )' + name.replace(/([\\.$?*|{}\\(\\)\\[\\]\\\\\\/\\+^])/g, '\\\\$1') + '=([^;]*)'));\n  return matches ? decodeURIComponent(matches[1]) : undefined;\n}\n\nexport default {\n  data() {\n    return {\n      loginin: '',\n      passwordin: '',\n      url: document.location.pathname,\n      up: {},\n      posts: {},\n      components_key_re: 0\n    };\n  },\n\n  created() {\n    axios.post('http://127.0.0.1:8000/personalpage', {\n      user_sid: document.cookie,\n      login: this.url,\n      login_from: getCookie('login')\n    }, {\n      withCredentials: true\n    }).then(response => {\n      this.up = response.data.up;\n      this.posts = response.data.postss;\n      console.log(this.posts);\n    }).catch(e => {\n      console.log(e);\n    });\n  },\n\n  methods: {\n    subsc() {\n      axios.post('http://127.0.0.1:8000/subscribator', {\n        user_sid: document.cookie,\n        login: this.url,\n        login_from: getCookie('login')\n      }, {\n        withCredentials: true\n      }).then(response => {\n        console.log(response.data);\n\n        if (String(response.data) === 'true') {\n          location.reload();\n        }\n      }).catch(e => {\n        console.log(e);\n      });\n    },\n\n    created_post_in() {\n      $('body').css({\n        'background-color': 'rgba(0, 0, 0, 0.315)',\n        'transition-duration': '0.4s',\n        opacity: '1'\n      });\n      $('.logo').css({\n        'background-color': 'rgba(0, 0, 0, 0.005)',\n        'transition-duration': '0.6s'\n      });\n      $('.btn').css({\n        'background-color': 'rgba(0, 0, 0, 0.005)',\n        'transition-duration': '0.6s'\n      });\n      $('.menu_new_post').fadeToggle();\n      $('.close').fadeToggle();\n    },\n\n    created_post_out() {\n      $('body').css({\n        'background-color': 'rgba(245, 245, 245, 0.85)',\n        'transition-duration': '0.4s',\n        opacity: '1'\n      });\n      $('.logo').css({\n        'background-color': 'rgba(245, 245, 245, 0.85)',\n        'transition-duration': '0.6s'\n      });\n      $('.btn').css({\n        'background-color': 'rgba(245, 245, 245, 0.85)',\n        'transition-duration': '0.6s'\n      });\n      $('.menu_new_post').fadeOut();\n      $('.close').fadeOut();\n    },\n\n    upload_file() {}\n\n  }\n};","map":{"version":3,"mappings":"AAgkBA,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,CAAP,MAAc,QAAd;;AAEA,SAASC,SAAT,CAAoBC,IAApB,EAA0B;EACxB,MAAMC,OAAM,GAAIC,QAAQ,CAACC,MAAT,CAAgBC,KAAhB,CAAsB,IAAIC,MAAJ,CACpC,aAAaL,IAAI,CAACM,OAAL,CAAa,8BAAb,EAA6C,MAA7C,CAAb,GAAoE,UADhC,CAAtB,CAAhB;EAGA,OAAOL,OAAM,GAAIM,kBAAkB,CAACN,OAAO,CAAC,CAAD,CAAR,CAAtB,GAAqCO,SAAlD;AACF;;AAEA,eAAe;EACbC,IAAG,GAAK;IACN,OAAO;MACLC,OAAO,EAAE,EADJ;MAELC,UAAU,EAAE,EAFP;MAGLC,GAAG,EAAEV,QAAQ,CAACW,QAAT,CAAkBC,QAHlB;MAILC,EAAE,EAAE,EAJC;MAKLC,KAAK,EAAE,EALF;MAMLC,iBAAiB,EAAE;IANd,CAAP;EAQD,CAVY;;EAWbC,OAAM,GAAK;IACTrB,KAAK,CAACsB,IAAN,CACE,oCADF,EAEE;MACEC,QAAQ,EAAElB,QAAQ,CAACC,MADrB;MAEEkB,KAAK,EAAE,KAAKT,GAFd;MAGEU,UAAU,EAAEvB,SAAS,CAAC,OAAD;IAHvB,CAFF,EAOE;MACEwB,eAAe,EAAE;IADnB,CAPF,EAWGC,IAXH,CAWSC,QAAD,IAAc;MAClB,KAAKV,EAAL,GAAUU,QAAQ,CAAChB,IAAT,CAAcM,EAAxB;MACA,KAAKC,KAAL,GAAaS,QAAQ,CAAChB,IAAT,CAAciB,MAA3B;MACAC,OAAO,CAACC,GAAR,CAAY,KAAKZ,KAAjB;IACD,CAfH,EAgBGa,KAhBH,CAgBUC,CAAD,IAAO;MACZH,OAAO,CAACC,GAAR,CAAYE,CAAZ;IACD,CAlBH;EAmBD,CA/BY;;EAgCbC,OAAO,EAAE;IACPC,KAAI,GAAK;MACPnC,KAAK,CAACsB,IAAN,CACE,oCADF,EAEE;QACEC,QAAQ,EAAElB,QAAQ,CAACC,MADrB;QAEEkB,KAAK,EAAE,KAAKT,GAFd;QAGEU,UAAU,EAAEvB,SAAS,CAAC,OAAD;MAHvB,CAFF,EAOE;QACEwB,eAAe,EAAE;MADnB,CAPF,EAWGC,IAXH,CAWSC,QAAD,IAAc;QAClBE,OAAO,CAACC,GAAR,CAAYH,QAAQ,CAAChB,IAArB;;QACA,IAAIwB,MAAM,CAACR,QAAQ,CAAChB,IAAV,CAAN,KAA0B,MAA9B,EAAsC;UACpCI,QAAQ,CAACqB,MAAT;QACF;MACD,CAhBH,EAiBGL,KAjBH,CAiBUC,CAAD,IAAO;QACZH,OAAO,CAACC,GAAR,CAAYE,CAAZ;MACD,CAnBH;IAoBD,CAtBM;;IAuBPK,eAAc,GAAK;MACjBrC,CAAC,CAAC,MAAD,CAAD,CAAUsC,GA