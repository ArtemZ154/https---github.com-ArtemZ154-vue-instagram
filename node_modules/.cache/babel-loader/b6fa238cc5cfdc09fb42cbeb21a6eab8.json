n}\r\n\r\n.login {\r\n    width: 260px;\r\n    margin: 0 auto;\r\n    text-align: center;\r\n    margin-top: 40px;\r\n}\r\n\r\n.login p {\r\n    font-family: 'Montserrat_Black';\r\n    font-size: 11px;\r\n    color: #000000;\r\n}\r\n</style>\r\n\r\n<script>\r\nimport axios from 'axios'\r\n\r\nexport default {\r\n  data: () => ({\r\n    posts: {},\r\n    errors: []\r\n  }),\r\n  methods: {\r\n    flogin () {\r\n      axios({\r\n        method: 'post',\r\n        url: 'http://127.0.0.1/login_s',\r\n        data: {\r\n          firstName: this.loginin,\r\n          lastName: this.passwordin\r\n        }\r\n      })\r\n        .then(response => {\r\n          console.log(response)\r\n          if (response == 'log_true') {\r\n            location.href = '/'\r\n          } if (response == 'true') {\r\n            location.href = '/'\r\n          }\r\n        })\r\n        .catch(e => {\r\n          this.errors.push(e)\r\n        })\r\n    }\r\n  }\r\n}\r\n</script>\r\n"]},"metadata":{},"sourceType":"module"}